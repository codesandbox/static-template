{
  "version": 2,
  "files": [
    ".index.html",
    "./static/about_me.html",
    "./static/fun_with_forms.html",
    "./static/404.html",
    "package.json"
  ],
  "builds": [
    {
      "src": "./static",
      "use": "@now/static"
    },
    {
      "src": "./index.html",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/home",
      "dest": "./index.html"
    },
    {
      "src": "/about",
      "dest": "./static/about_me.html"
    },
    {
      "src": "/forms",
      "dest": "./static/fun_with_forms.html"
    },
    {
      "src": "/*",
      "dest": "./static/404.html"
    }
  ]
}

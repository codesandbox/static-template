{
  "version": 2,
  "files": [
    "index.html",
    "fun_with_forms.html",
    "about_me.html",
    "package.json"
  ],
  "builds": [
    {
      "src": "index.html",
      "use": "@now/static"
    }
  ],
  "routes": [
    {
      "src": "/home",
      "dest": "/index.html"
    },
    {
      "src": "/about",
      "dest": "/about_me.html"
    },
    {
      "src": "/forms",
      "dest": "/fun_with_forms.html"
    },
    {
      "src": "/*",
      "dest": "/404.html"
    }
  ]
}
